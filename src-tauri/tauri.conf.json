{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "my-project",
  "version": "0.1.0",
  "identifier": "com.myproject.screenrecorder",
  "build": {
  "frontendDist": "../dist",
  "devUrl": null
},
  "app": {
    "windows": [
      {
        "title": "my-project",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
